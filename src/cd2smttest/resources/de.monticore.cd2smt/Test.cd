classdiagram Test {
  enum PositionKind{fullTime, partTime, external;}

  class Employee {
    PositionKind kind;
    int id;
    public void completeTask(Task task);
  }

  class Task {
    String startDate;
  }

  class Manager extends Employee;

  association [*] Employee (manages) -- (managedBy) Manager [0..1];
  association [1] Employee  --  Task [0..1];
}
