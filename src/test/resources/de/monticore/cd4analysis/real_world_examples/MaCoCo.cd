  // Konto -> *MailAlert

  class MailAlert {
    String name;
    String alertCondition;
    String mailBody;
    int repetitions;
    ZonedDateTime firstAlert;
    ZonedDateTime lastAlert;
    Optional<String> receipients;
    Optional<String> ccs;
    Optional<String> bccs;
    long a1;
    long a2;
    boolean flag1;
    boolean flag2;
    String s1;
    String s2;
  }

