package de.monticore.life;

classdiagram MyLife {
  abstract class Person implements CorporateEntity {
    String name;
    int age;
    Date birthday;
    List<String> nickNames;
  }

  class Address;
  class PhoneNumber;

  package uni {
    class Student extends Person {
      StudentStatus status;
      -> Address [1..*] {ordered};
    }

    enum StudentStatus {
      ENROLLED,
      FINISHED;
    }

    class Grades;

    composition Student -> Grades [*];
    association phonebook uni.Student [String] -> PhoneNumber;
  }
  association [0..1] Person (parent) <-> (child) de.monticore.life.Person [*];

  interface CorporateEntity {
     int vatNumber;
  }
}
