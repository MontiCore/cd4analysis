package de.monticore.cd4code;

classdiagram CD4CodeLanguageTeaser {

  interface IObjectClass;

  public abstract class Person implements IObjectClass {
    protected String name;
    public Person(String name);
    public String getName();
  }

  class Student extends Person {
    public Student(String name, StudentStatus status);
    StudentStatus status;
  }

  enum StudentStatus {
    REGISTERED,
    EXMATRICULATED,
    COMPLETED;
  }

  association Person -> (supervises) Student [*];

}
