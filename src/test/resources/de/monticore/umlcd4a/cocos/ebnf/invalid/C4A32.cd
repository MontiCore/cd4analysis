classdiagram C4A32 {
  interface A1;
  interface B1;
  read-only association A1 -> (foo) B1 [0..1];

  class A2 implements A1;
  class B2 implements B1;
  association A2 -> (foo) B2 [1..*]; // error, because 1..* is subset of 0..1
}
