package de.monticore.cd4analysis.parser;

classdiagram MyLife {

  public abstract class Person implements CorporateEntity {
    protected String name;
    int age;
    Date birthday;
    List<String> nickNames;
  }

  class Student extends Person {
    StudentStatus status;
    -> Address [1..*] {ordered};
  }

  enum StudentStatus {
    ENROLLED,
    FINISHED;
  }

  composition Student -> Grades [*];
  association [0..2] Person (parent) <-> (child) Person [*];
  association phonebook Person [String] -> PhoneNumber;

  interface CorporateEntity {
     int vatNumber;
  }
}
