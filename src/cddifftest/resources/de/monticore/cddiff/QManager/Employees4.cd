package man;
import java.util.Date;

classdiagram Employees4 {
  enum PositionKind{fullTime, partTime, external;}

  package emp {
    class Employee {
      PositionKind kind;
    }

    class Manager extends Employee;
  }

  class Task {
    Date startDate;
  }

  association [*] man.emp.Employee (manages) -> (managedBy) man.emp.Manager [0..1];
  association [1] man.emp.Employee  --  Task [0..1];
}
