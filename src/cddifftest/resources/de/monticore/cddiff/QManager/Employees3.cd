package man;
import java.util.Date;

classdiagram Employees3 {
  enum PositionKind{fullTime, partTime;}

  package emp {
    class Employee {
      PositionKind kind;
    }

    class Manager;
  }

  class Task {
    Date startDate;
  }

  association [*] emp.Employee (manages) -> (managedBy) emp.Manager [0..1];
  association [1] emp.Employee  --  Task [*];
}
