classdiagram MyLectures {
  class Student {
    int studentID;
  }
  class Professor {
    int salary;
    String chair;
  }
  class Lecture{
    int credits;
  }

  association [1] Professor (lecturer) <-> (holds) Lecture;
  association attendance Student -- Lecture [*];
}
