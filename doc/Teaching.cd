classdiagram MyTeaching {
	class Professor{
	  String chair;
	  String mail;
	}
	class Lecture {
    int credits;
  }
  class Student {
    long matrNr;
  }

	association [1] Professor (lecturer) <-> (holds) Lecture;
	association attendance [*] Lecture <- Student;
}
