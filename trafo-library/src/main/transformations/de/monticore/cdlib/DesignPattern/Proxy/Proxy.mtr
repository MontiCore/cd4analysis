package de.monticore.cdlib.designpatterns.proxy.tf;
transformation Proxy {
  classdiagram $Classdiagram{
      class $realClassName;

    [[ :- interface $interfaceName; ]]

      [[ :- class $proxyName implements $interfaceName; ]]

    [[ :- association  $proxyName -> $realClassName; ]]
  }
}