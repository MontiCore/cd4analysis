package de.monticore.cdlib.refactoring.extractsuperclass.method.tf;
transformation AdaptSubclassMethod {
  classdiagram $_{
    $Sub [[	class $subclass [[ :- extends $newParent ]] {
      [[ CDMethod $A  :- ]]
    }]]
  }

  where{
    ($Sub.getSuperclassList().isEmpty())&&
    (!$subclass.equals($newParent))
  }
}
