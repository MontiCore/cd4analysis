package de.monticore.cdlib.refactoring.extractsuperclass.attribute.tf;
transformation AdaptSubclassAttribute {
  classdiagram $_{
    $Sub [[	class $subclass [[ :- extends $newParent ]] {
      [[ CDAttribute $A  :- ]]
    } ]]
  }

  where{
    ($Sub.getSuperclassList().isEmpty())&&
    (!$subclass.equals($newParent))
  }
}
