package de.monticore.cdlib.refactoring.delete.tf;
transformation DeleteClassInInheritance {
  classdiagram $_ {
    class $superclass;

    class $classToDelete extends $superclass;

    class $subclass extends [[ $classToDelete :- $superclass ]] ;
  }
}
