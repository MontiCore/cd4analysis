package de.monticore.cdlib.refactoring.collapsehierarchy.method.tf;
transformation CollapseHierarchyMethod {
  classdiagram $_{


    class $superclass1{
       [[CDMethod $A1 :-]]
    }

    list [[class $_ extends $superclass1{
      not [[ CDMethod $A2 ]]
      [[ :- CDMethod $A1]]
    }]]


  }

  where { $A2.deepEquals($A1)}
}
