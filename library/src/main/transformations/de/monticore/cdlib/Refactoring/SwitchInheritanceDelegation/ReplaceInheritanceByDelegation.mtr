package de.monticore.cdlib.refactoring.switchinheritancedelegation.tf;
transformation ReplaceInheritanceByDelegation {
  classdiagram $_{
    class $subclassName extends [[ $superclass :- ]] ;

    [[ :- association $subclassName -> $superclass; ]]
  }
}