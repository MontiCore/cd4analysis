package de.monticore.cdlib.refactoring.extractintermediateclass.additionalclasses.tf;
transformation ExtractIntermediateClassAdaptSubclassAttribute {
  classdiagram $_{
    class $subclass extends [[ $parent :- $newParent ]] {
      [[ CDAttribute $A  :- ]]
    }
  }
}
