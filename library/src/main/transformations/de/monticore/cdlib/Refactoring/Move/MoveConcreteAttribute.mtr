package de.monticore.cdlib.refactoring.move.tf;
transformation MoveConcreteAttribute {
  classdiagram $_ {
     class $startClassName {
      [[ CDAttribute $M1 [[ MCType $type $name; ]] :- ]]
    }

    class $destinationClassName{
      not [[ MCType $_ $name; ]]

      [[ :- CDAttribute $M1 ]]
    }
  }
}
