package de.monticore.cdlib.refactoring.pullup.attribute.tf;
transformation PullUpAttributes {
  classdiagram $_ {
    class $parent {
      [[ :- CDAttribute $A1 ]]
    }
    class $_ extends $parent{
      [[  CDAttribute $A1 :- ]]
    }
    list[[  class $_ extends $parent {
              [[  CDAttribute $A2  :- ]]
            }
    ]]
    not [[ class $_ extends $parent; ]]
  }
  where {  $A1.deepEquals($A2) }
}
