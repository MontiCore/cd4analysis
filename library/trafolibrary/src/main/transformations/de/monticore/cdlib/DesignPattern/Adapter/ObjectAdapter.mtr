package de.monticore.cdlib.designpatterns.adapter.tf;
transformation ObjectAdapter {
  classdiagram $_ {
    class $adaptee;

      [[ :- class $adapterName implements $targetName; ]]

      [[ :- interface $targetName; ]]

      [[ :- association adaptee $adapterName -> $adapteeName ; ]]
  }
}
