package de.monticore.cdlib.refactoring.pushdown.attribute.tf;
transformation CopyAttributeToSubclass {
  classdiagram $_ {
    class $superclass {
       $attribute [[ MCType $_ $name; ]]
    }

    class $className extends $superclass{
      not [[ MCType $_ $name; ]]
        [[ :- CDAttribute $attribute ]]
    }
  }
}
